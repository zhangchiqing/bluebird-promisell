  "peerDependencies": {
    "bluebird": ">=2.1.0"
  },
  "devDependencies": {
    "bluebird": "^3.3.5",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "doctest": "^0.10.0",
    "istanbul": "^0.4.2",
    "jsverify": "^0.7.1",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.0.0",
    "transcribe": "^0.3.0",
    "xyz": "^0.5.0"
