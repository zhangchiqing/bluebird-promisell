  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhangchiqing/bluebird-promisell.git"
  },
  "files": [
    "/LICENSE",
    "/index.js",
    "/package.json"
  ],
  "author": "Leo Zhang <zhangchiqing@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhangchiqing/bluebird-promisell/issues"
  },
